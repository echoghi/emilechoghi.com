{
    "presets": [
        "@babel/preset-react",
        [
            "@babel/env",
            {
                "targets": {
                    "browsers": "last 2 versions"
                },
                "loose": true,
                "modules": false
            }
        ]
    ],
    "plugins": ["@babel/plugin-proposal-class-properties", "@babel/plugin-syntax-dynamic-import"]
}
